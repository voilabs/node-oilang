{
    "name": "@voilabs/oilang",
    "description": "A robust internationalization (i18n) handling library designed for performance and flexibility.",
    "version": "0.0.18",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "scripts": {
        "build": "tsc",
        "prepublishOnly": "npm run build"
    },
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.js"
        },
        "./adapters": {
            "types": "./dist/adapters/index.d.ts",
            "import": "./dist/adapters/index.js"
        },
        "./stores": {
            "types": "./dist/stores/index.d.ts",
            "import": "./dist/stores/index.js"
        },
        "./handlers": {
            "types": "./dist/handlers/index.d.ts",
            "import": "./dist/handlers/index.js"
        },
        "./utils": {
            "types": "./dist/utils/index.d.ts",
            "import": "./dist/utils/index.js"
        },
        "./types": {
            "types": "./dist/types.d.ts"
        }
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@types/node": "^25.2.3",
        "@types/pg": "^8.16.0"
    },
    "publishConfig": {
        "access": "public"
    },
    "keywords": [
        "i18n",
        "full-stack",
        "realtime"
    ],
    "peerDependencies": {
        "typescript": "^5.9.3"
    },
    "homepage": "https://oilang.com",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/voilabs/node-oilang.git"
    },
    "dependencies": {
        "axios": "^1.13.5",
        "elysia": "^1.4.25",
        "ioredis": "^5.9.3",
        "pg": "^8.18.0"
    },
    "files": [
        "dist"
    ]
}
